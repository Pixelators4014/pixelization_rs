{
  "id": "pixelization-dev-container",
  "version": "0.1.0",
  "image": "pixelization",
  "build.context": "..",
  "runArgs": ["-v $HOME/workspaces/isaac_ros-dev:/workspaces/isaac_ros-dev", "-e NVIDIA_VISIBLE_DEVICES=all", "--network host", "-v /dev/*:/dev/*", "-v /etc/localtime:/etc/localtime:ro", "--runtime nvidia", "--workdir /workspaces/isaac_ros-dev"]
}